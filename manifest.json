{
    "manifest_version": 3,
    "name": "RL Data Collector",
    "version": "2.0",
    "description": "Logs user telemetry across all tabs.",
    "permissions": ["storage", "activeTab", "scripting", "unlimitedStorage"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
}